{
  "name": "twelvety",
  "version": "1.2.2",
  "description": "A pre-configured, super-fast Eleventy starter project",
  "scripts": {
    "clean": "rimraf dist .twelvety.cache",
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "watch": "cross-env ELEVENTY_ENV=development eleventy --watch",
    "serve": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "watch-serve": "cross-env ELEVENTY_ENV=development eleventy --watch --serve"
  },
  "repository": "https://github.com/gregives/twelvety",
  "keywords": [
    "eleventy",
    "starter",
    "project"
  ],
  "author": "Greg Ives",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "autoprefixer": "^10.4.21",
    "browserify": "^17.0.1",
    "clean-css": "^5.3.3",
    "critical": "^7.2.1",
    "cross-env": "^7.0.3",
    "deasync": "^0.1.30",
    "html-minifier": "^4.0.0",
    "instant.page": "^5.2.0",
    "js-beautify": "^1.15.4",
    "jsonfile": "^6.2.0",
    "module-alias": "^2.2.3",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.92.1",
    "sharp": "^0.34.3",
    "tinyify": "^3.1.0",
    "uglify-js": "^3.19.3"
  }
}
